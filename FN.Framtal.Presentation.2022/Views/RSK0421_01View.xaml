            </Border>
            <Border Grid.Row="0" Grid.Column="1" Background="Transparent" BorderThickness="0 1 1 1">
                <TextBox BorderThickness="0" />
            </Border>
            <Border Grid.Row="0" Grid.Column="2" Background="Transparent" BorderThickness="0 1 0 1">
                <TextBlock  Padding="5" FontSize="11px">Nr. verkefnis<LineBreak/>hjá Rannís
                </TextBlock>
            </Border>
            <Border Grid.Row="0" Grid.Column="3" Background="Transparent" BorderThickness="0 1 1 1">
                <TextBox BorderThickness="0" />
            </Border>
        </Grid>
        <!--#endregion-->
