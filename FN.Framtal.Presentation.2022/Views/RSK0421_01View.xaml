                <TextBlock Padding="10 0 10 0" FontSize="11px" TextAlignment="Right" FontWeight="Bold" VerticalAlignment="Center" TextWrapping="Wrap">
                    Samtals
                </TextBlock>
            </Border>
            <Border Grid.Row="0" Grid.Column="1" Background="Transparent" BorderThickness="0" Padding="3">
                <Polygon Points="0 0 16 12 0 20"  Fill="{DynamicResource DarkBrush}" />
            </Border>
            
            <Border Grid.Row="0" Grid.Column="2" Background="Transparent" BorderThickness="2 1 0 2" >
                <TextBlock Padding="5" FontSize="11px" FontWeight="Bold"  VerticalAlignment="Center" TextWrapping="Wrap">
                    kr.
                </TextBlock>
