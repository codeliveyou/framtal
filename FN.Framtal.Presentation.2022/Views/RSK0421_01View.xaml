            </Border>
            <Border Grid.Row="0" Grid.Column="2" Background="Transparent" BorderThickness="0 0 0 1" >
                <RadioButton 
                    Grid.Row="1"
                    Grid.Column="1"
                    x:Name="Company_size_option2" 
                    Margin="5" 
                    VerticalAlignment="Center"
                    FontWeight="Bold"
                    GroupName="Company size" 
                    Content="Meðalstórt" />
            </Border>
            <Border Grid.Row="0" Grid.Column="3" Background="Transparent" BorderThickness="0 0 0 1" >
                <RadioButton 
