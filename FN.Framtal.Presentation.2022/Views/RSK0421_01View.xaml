            <Grid.RowDefinitions>
                <RowDefinition Height="30"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="10"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="24"/>
                <ColumnDefinition Width="26"/>
                <ColumnDefinition Width="150"/>
            </Grid.ColumnDefinitions>

            <TextBlock 
                Grid.Row="0" 
                Grid.Column="0" 
                Grid.ColumnSpan="4" 
                FontWeight="Bold" FontSize="14px" >
                Aðkeypt rannsóknar- og þróunarvinna, sem staðfest hefur verið af Rannís
            </TextBlock>

            <Border 
                Grid.Row="1" Grid.Column="0" 
                Grid.ColumnSpan="4" 
                Background="Transparent" 
                BorderThickness="1 1 1 1" 
                DataContext="{Binding RSK421_ADKEYPT_VINNA}">
                <DataGrid
                    BorderThickness="0" 
                    Grid.Row="1"
                    AutoGenerateColumns="False"
                    BorderBrush="Black"
                    ItemsSource="{Binding}">
                    <DataGrid.Columns>
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding Path=KENNITALA}"
                            Header="Kennitala framteljanda/maka" />
                        <DataGridTextColumn
                            Width="*"
                            Binding="{Binding Path=TEKJUAR}"
                            Header="Tekjuár" />
