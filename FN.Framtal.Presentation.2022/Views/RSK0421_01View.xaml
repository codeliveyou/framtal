                    Margin="5" 
                    VerticalAlignment="Center"
                    FontWeight="Bold"
                    GroupName="Project Type" 
                    Content="Rannsóknarverkefni" />
            </Border>
        </Grid>
        <!--#endregion-->
        <!-- end Row#4 -->

        <!-- begin Row#5 -->
        <!--#region-->
        <Grid Grid.Column="2" Grid.Row="5">
