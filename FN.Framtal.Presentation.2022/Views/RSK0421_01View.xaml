            <Border Grid.Row="0" Grid.Column="2" Background="Transparent" BorderThickness="1 0 0 1" >
                <TextBlock Padding="10 0 0 0" FontSize="11px"  VerticalAlignment="Center" TextWrapping="Wrap">
                    Eignarhluti framteljanda í verkefninu.
                </TextBlock>
            </Border>

            <Border Grid.Row="0" Grid.Column="3" Background="Transparent" BorderThickness="0 0 0 1">
                <controls:NumericTextBox BorderThickness="1 1 0 1" Margin="5 5 0 5" BorderBrush="Black" TextAlignment="Center"/>
            </Border>

            <Border Grid.Row="0" Grid.Column="4" Background="Transparent" BorderThickness="0 0 0 1">
                <controls:NumericTextBox BorderThickness="0 1 1 1" Text="%" Margin="0 5 0 5" BorderBrush="Black" TextAlignment="Center" Padding="2 5 5 2" IsReadOnly="True" ></controls:NumericTextBox>
            </Border>

            <Border Grid.Row="0" Grid.Column="5" Background="Transparent" BorderThickness="0 0 0 1" >
                <TextBlock Padding="10 0 0 0" FontSize="11px"  VerticalAlignment="Center" TextWrapping="Wrap">
                    Framteljandi skal á þessu eyðublaði tilgreina <Run FontWeight="Bold">heildarkostnað</Run> við verkefnið.
                </TextBlock>
            </Border>
            <Border Grid.Row="0" Grid.Column="6" Background="Transparent" BorderThickness="0 0 1 1" >
                <TextBlock Padding="10 0 0 0" FontSize="11px"  VerticalAlignment="Center" TextWrapping="Wrap">
                    Athugið! Jafnframt skal skila <Run FontWeight="Bold">RSK 4.22.</Run>
                </TextBlock>
            </Border>
        </Grid>
