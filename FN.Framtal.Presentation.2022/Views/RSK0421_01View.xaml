            <Grid.RowDefinitions>
                <RowDefinition Height="40"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="20"/>
                <RowDefinition Height="20"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1.2*"/>
                <ColumnDefinition Width="1.5*"/>
            </Grid.ColumnDefinitions>

            <TextBlock Grid.Column="1"  Grid.Row="0" 
                HorizontalAlignment="right" 
                VerticalAlignment="Center" 
                Grid.ColumnSpan="2"
                FontSize="26" 
                FontWeight="Bold"
                TextAlignment="Right">
                Greinargerð um skattfrádrátt
            </TextBlock>
            <TextBlock Grid.Column="1"  Grid.Row="1" 
                HorizontalAlignment="right" 
                VerticalAlignment="Center" 
                Grid.ColumnSpan="2"
                FontSize="14px" 
                FontWeight="Bold"
                TextAlignment="Right">
                skv. lögum um stuðning við nýsköpunarfyrirtæki
            </TextBlock>
            <TextBlock Grid.Column="1"  Grid.Row="3" 
                HorizontalAlignment="right" 
                VerticalAlignment="Bottom" 
                Grid.ColumnSpan="2"
                FontSize="13px" 
                TextAlignment="Right">
                Fylgiskjal með skattframtali rekstraraðila 2022 RSK 1.04
            </TextBlock>
        </Grid>
        <!--#endregion-->
        <!-- end #Row1 -->

        <!-- begin Row#2 -->
        <!--#region-->
        <Grid Grid.Column="2" Grid.Row="2">
            <Grid.RowDefinitions>
                <RowDefinition Height="10"/>
                <RowDefinition Height="40"/>
                <RowDefinition Height="40"/>
                <RowDefinition Height="10"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="2*"/>
                <ColumnDefinition Width="240"/>
            </Grid.ColumnDefinitions>

