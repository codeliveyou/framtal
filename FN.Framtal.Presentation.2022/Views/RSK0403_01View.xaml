                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Margin="5,3" FontSize="9">Notkunartími<LineBreak/>Frá
                            </TextBlock>
                            <TextBlock Grid.Row="0" Grid.Column="1" Margin="5,3" FontSize="9" VerticalAlignment="Bottom">Til
                            </TextBlock>
                            <Border Grid.Row="1" Grid.Column="0" Padding="3" Margin="3,0" Background="{StaticResource DarkBrush}" BorderThickness="1">
                                <controls:NumericTextBox Name="T403_NOTKUN_FRA"  BorderThickness="0" Text="{Binding T403_NOTKUN_FRA}" />
                            </Border>
                            <Border Grid.Row="1" Grid.Column="1" Padding="3" Margin="3,0" Background="{StaticResource DarkBrush}" BorderThickness="1">
                                <controls:NumericTextBox Name="T403_NOTKUN_TIL"  BorderThickness="0" Text="{Binding T403_NOTKUN_TIL}" />
                            </Border>
