                                    <controls:NumericTextBox Name="N403_EKNIR_KM_SAMTALS"  BorderThickness="0" Text="{Binding N403_EKNIR_KM_SAMTALS}" />
                                </Border >
                                <TextBlock Grid.Column="2" Margin="3,0" VerticalAlignment="Bottom" >Km</TextBlock>
                            </Grid>
                        </Grid>
                    </Border>



                    <!--<Border Grid.Row="1" Grid.Column="3">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"></RowDefinition>
                                <RowDefinition Height="auto"></RowDefinition>
                            </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="3" FontSize="9"><LineBreak/>Afskrift, sbr. ao neoan</TextBlock>
                            <Border Grid.Row="1" Grid.Column="0" Padding="3" Margin="3,0" Background="{StaticResource DarkBrush}" BorderThickness="1">
                                <controls:NumericTextBox  BorderThickness="0" Text="{Binding NTB_02}" />
                            </Border>
                        </Grid>
                    </Border>-->



                    <Border Grid.Row="1" Grid.Column="4" BorderThickness="1,1,1,1" Margin="0,0,0,0">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="9.357"></RowDefinition>
                                <RowDefinition Height="20.583"/>
                                <RowDefinition Height="auto"></RowDefinition>
                            </Grid.RowDefinitions>

                            <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" Margin="0,0,0,24" Grid.RowSpan="3">
                                <TextBlock  Margin="5,3" FontSize="9">Akstur í eigin þágu, <LineBreak/>þ.m.t. til og frá vinnu</TextBlock>
                            </StackPanel>
                            <Grid Grid.Row="2" Grid.Column="0" >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="auto" ></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Border Grid.Column="0" Padding="3" Margin="3,0" Background="{StaticResource DarkBrush}" BorderThickness="1">
                                    <controls:NumericTextBox Name="N403_AKSTUR_EIGINN"  BorderThickness="0" Text="{Binding N403_AKSTUR_EIGINN}" />
                                </Border >
                                <TextBlock Grid.Column="2" Margin="3,0" VerticalAlignment="Center" >Km</TextBlock>
                            </Grid>
                        </Grid>
                    </Border>

