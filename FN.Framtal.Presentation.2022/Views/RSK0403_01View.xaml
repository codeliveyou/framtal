                                <RowDefinition Height="auto"></RowDefinition>
                                <RowDefinition Height="auto"></RowDefinition>
                            </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="5,3" FontSize="9">Staða mælis í árslok<LineBreak/>eða þegar bifreið er seld</TextBlock>
