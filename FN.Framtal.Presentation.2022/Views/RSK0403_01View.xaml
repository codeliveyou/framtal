                                <RowDefinition Height="auto"></RowDefinition>
                                <RowDefinition Height="auto"></RowDefinition>
                            </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="5,3" FontSize="9">Tegund</TextBlock>
                            <Border Grid.Row="1" Grid.Column="0" Padding="3" Margin="3,3" Background="{StaticResource DarkBrush}" BorderThickness="1">
                                <controls:NumericTextBox Name="T403_TEGND_BILL"  BorderThickness="0" Text="{Binding T403_TEGND_BILL}" />
                            </Border>
