                                <controls:NumericTextBox Name="N403_STADA_LOK"  BorderThickness="0" Text="{Binding N403_STADA_LOK}" />
                            </Border>
                        </Grid>
                    </Border>

                    <Border Grid.Row="1" Grid.Column="2" >
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto"></RowDefinition>
                                <RowDefinition Height="auto"></RowDefinition>
                            </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="5,3" FontSize="9">Staða mælis í ársbyrjun<LineBreak/>eða þegar bifreið er keypt</TextBlock>
                            <Border Grid.Row="1" Grid.Column="0" Padding="3" Margin="3,0" Background="{StaticResource DarkBrush}" BorderThickness="1">
                                <controls:NumericTextBox Name="N403_STADA_BYRJUN"  BorderThickness="0" Text="{Binding N403_STADA_BYRJUN}" />
                            </Border>
                        </Grid>
                    </Border>


