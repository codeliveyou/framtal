<Application
    x:Class="FN.Framtal.Presentation._2022.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:FN.Framtal.Presentation._2022"
    xmlns:lib="clr-namespace:FN.Framtal.Presentation._2022.Lib">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/FN.Framtal.UserControls;component/Theme.xaml" />
                <lib:SkinResourceDictionary RedSource="Resources/Skins/RedSkin.xaml"
                                            BlueSource="Resources/Skins/BlueSkin.xaml"
                                            GreenSource="Resources/Skins/GreenSkin.xaml"
                                            YellowSource="Resources/Skins/YellowSkin.xaml"/>
                <ResourceDictionary Source="Resources/Theme.xaml"/>
            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
</Application>
